@import "../../common/vendor.wxss";
.page {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-align-content: flex-start;
  align-content: flex-start;
}
.swipper-content {
  width: 100%;
  height: 300rpx;
  background: #AEEEEE;
}
.swipper-img {
  width: 100%;
  height: 300rpx;
}
.line-content {
  width: 100%;
  height: 80rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.line {
  width: 36%;
  height: 2rpx;
  background: #BABABA;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.content {
  color: #BABABA;
  font-size: 32rpx;
  margin: 0 20rpx;
}